{
    "name": "GoPhish",
    "version": "1.0",
    "description": "Sends the selected URL to a popup",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": [
          "https://example.com/*"
        ],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "GoPhish.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  